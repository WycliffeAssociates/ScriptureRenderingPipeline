{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "default": "Warning",
            "Microsoft": "Warning",
            "Function": "Information"
        }
    },
    "extensions": {
        "serviceBus": {
            "messageHandlerOptions": {
                "maxConcurrentCalls": 2
            },
            "sessionHandlerOptions": {
                "maxConcurrentSessions": 2
            }
        }
    }
}