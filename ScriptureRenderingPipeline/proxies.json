{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "bielcatalog": {
      "matchCondition": { "route": "/v3/catalog.json" },
      "backendUri": "https://%CatalogServer%/v3/catalog.json"
    },
    "writerCatalog": {
      "matchCondition": { "route": "/v2/ts/catalog.json" },
      "backendUri": "https://%CatalogServer%/v2/ts/catalog.json"
    },
    "writerCatalogAndroid": {
      "matchCondition": { "route": "/ts/txt/2/catalog.json" },
      "backendUri": "https://%CatalogServer%/v2/ts/catalog.json"
    },
    "unfoldingWordCatalog": {
      "matchCondition": { "route": "/uw/txt/2/catalog.json" },
      "backendUri": "https://%CatalogServer%/uw/txt/2/catalog.json"
    }
  }
}
